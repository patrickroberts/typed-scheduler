{
  "name": "typed-scheduler",
  "version": "0.1.2",
  "description": "A Scheduler written in TypeScript for concurrency-limiting and rate-limiting",
  "main": "dst/scheduler.js",
  "repository": "github:patrickroberts/typed-scheduler",
  "scripts": {
    "compile": "rm -rf dst; tsc -p .",
    "lint": "tslint -p . --fix",
    "test": "npm run lint",
    "docs": "rm -rf docs; typedoc src/scheduler.ts",
    "build": "npm run compile && npm run test && npm run docs"
  },
  "keywords": [
    "async",
    "batch",
    "concurrency",
    "limiting",
    "promises",
    "queue",
    "rate",
    "scheduler",
    "task",
    "typescript"
  ],
  "author": "Patrick Roberts",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^11.13.0",
    "tslint": "^5.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.2",
    "typedoc-plugin-sourcefile-url": "^1.0.4",
    "typescript": "^3.4.2"
  }
}
