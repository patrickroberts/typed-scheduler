{
  "name": "typed-scheduler",
  "version": "0.0.0",
  "description": "A TaskScheduler module written in TypeScript for concurrency-limiting and rate-limiting",
  "main": "dst/TaskScheduler.js",
  "repository": "github:patrickroberts/typed-scheduler",
  "scripts": {
    "compile": "rm -rf dst; tsc -p .",
    "tslint": "tslint -p . --fix && tslint --fix dst/*.d.ts",
    "eslint": "eslint --fix dst",
    "lint": "npm run tslint && npm run eslint",
    "test": "npm run lint",
    "docs": "rm -rf docs; typedoc src/TaskScheduler.ts",
    "build": "npm run compile && npm run test && npm run docs"
  },
  "keywords": [],
  "author": "Patrick Roberts",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^11.13.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "tslint": "^5.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.2",
    "typedoc-plugin-sourcefile-url": "^1.0.4",
    "typescript": "^3.4.2"
  }
}
